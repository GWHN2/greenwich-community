{
  "name": "nextjs-difinity",
  "version": "0.1.0",
  "description": "Next.js Internet Computer Starter Template",
  "private": true,
  "keywords": [
    "nextjs",
    "internet computer",
    "icp",
    "starter",
    "dfinity"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next export",
    "sync:nft": "DFX_NETWORK=local && rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/nft --exclude='idl/' --exclude='*.wasm' --delete src/declarations",
    "sync:token": "DFX_NETWORK=local && rsync -avr .dfx/$(echo ${DFX_NETWORK:-'**'})/canisters/token --exclude='idl/' --exclude='*.wasm' --delete src/declarations"
  },
  "dependencies": {
    "@dfinity/agent": "^0.12.1",
    "@dfinity/candid": "^0.12.1",
    "@dfinity/principal": "^0.12.1",
    "@heroicons/react": "^2.0.1",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "buffer": "^6.0.3",
    "compressorjs": "^1.1.1",
    "events": "^3.3.0",
    "ipfs-http-client": "^57.0.3",
    "markdown-it": "^13.0.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.34.2",
    "react-markdown-editor-lite": "^1.3.3",
    "react-query": "^3.39.2",
    "react-scroll": "^1.8.7",
    "react-table": "^7.8.0",
    "react-toastify": "^9.0.8",
    "recoil": "^0.7.4",
    "recoil-persist": "^4.2.0",
    "sass": "^1.54.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-scroll": "^1.8.4",
    "@types/react-table": "^7.7.12",
    "@types/webpack-env": "^1.18.0",
    "autoprefixer": "^10.4.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
